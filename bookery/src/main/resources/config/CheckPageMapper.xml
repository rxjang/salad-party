<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="co.salpa.bookery.model.CheckPageDao">

	<select id="selectAll" resultType="CheckPageBean">
		select * from checkpage
	</select>

	<update id="updateOne" parameterType="CheckPageBean">
		update checkpage set
		actualpage=#{actualpage} where date = #{date} and
		study_id=${study_id} and deleted = 0
	</update>

</mapper>